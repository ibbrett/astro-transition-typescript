---
const baseUrl = await import.meta.env.BASE_URL;
const { page, posts, label } = Astro.props;
---

<div class="title">
  {posts.map((post) => (<nobr><a href={`${baseUrl}/${page}/${post.slug}/`}>[ {post.slug} ]</a></nobr><span>&nbsp;&nbsp;</span>))}
  <hr />
</div>
<div class="label">{label}:</div>
