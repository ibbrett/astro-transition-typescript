{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "checkJs": true,
    "allowJs": true,
    "outDir": "build-es3",
    "target": "es3",
    "noEmit": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": ["src/typescript/**/*"]
}
