{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "checkJs": true,
    "allowJs": true,
    "outDir": "build-es6",
    "target": "es6",
    "noEmit": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": ["src/typescript/**/*"]
}
